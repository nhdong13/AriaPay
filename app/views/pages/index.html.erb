<main id="content" role="main">
  <!-- Hero -->
    <div class="gradient-x-overlay-image">
      <!-- Image Background -->
      <div class="image-bg-content"
           style="background-image: url(<%= image_path "house_1.png"%>);">
      </div>
      <!-- End Image Background -->

      <div class="position-relative zi-2">
        <!-- Content -->
        <div class="d-md-flex">
          <div class="container d-md-flex align-items-md-center text-center content-space-3 content-space-t-md-4 content-space-t-lg-3" style="height: 630px">
            <div class="w-lg-75 mx-lg-auto">
              <!-- Heading -->
              <div class="mb-4">
                <h1 class="display-4 text-white mb-3">Change is coming</h1>
                <p class="lead text-white fs-4">AriaPay is revolutionising service charge management for the property market</p>
                <p class="lead text-white fs-5 fw-bold mb-0 mt-8">Register interest today</p>
              </div>
              <!-- End Title & Description -->

              <!-- Input Card -->
              <form id="register">
                <div class="input-card input-card-sm">
                  <div class="input-card-form">
                    <label for="nameRegisterForm" class="form-label visually-hidden">Enter your name</label>
                    <input type="text" class="form-control form-control-lg" id="nameRegisterForm" placeholder="Your name" aria-label="Your name">
                  </div>
                  <div class="input-card-form">
                    <label for="emailRegisterForm" class="form-label visually-hidden">Enter email</label>
                    <input type="text" class="form-control form-control-lg" id="emailRegisterForm" placeholder="Your email" aria-label="Your email">
                  </div>
                  <button type="button" class="btn btn-primary btn-lg">Get started</button>
                </div>
              </form>
              <!-- End Input Card -->
            </div>
          </div>
        </div>
        <!-- End Content -->

        <!-- Clients -->
        <div class="position-absolute bottom-0 start-0 end-0">
          <div class="container py-5">
            <!-- Swiper Slider -->
            <div class="js-swiper-hero-clients swiper-container text-center">
              <div class="swiper-wrapper justify-content-center align-items-center">
                <!-- Slide -->
                <div class="swiper-slide">
                  <%= image_tag('capital-dock.png', alt: "Logo", size: "122x45") %>
                </div>
                <!-- End Slide -->

                <div class="swiper-slide">
                  <%= image_tag('twinlite.png', alt: "Logo", size: "52x60") %>
                </div>
                <!-- End Slide -->

                <div class="swiper-slide">
                  <%= image_tag('cairn.png', alt: "Logo", size: "80x20") %>
                </div>
                <!-- End Slide -->
              </div>
            </div>
            <!-- End Swiper Slider -->
          </div>
        </div>
        <!-- End Clients -->
      </div>
    </div>
    <!-- End Hero -->

    <!-- Intro Blocks -->
    <div class="overflow-hidden">
      <div class="container position-relative content-space-1 content-space-t-lg-3">
        <!-- Heading -->
        <div class="w-md-75 w-lg-50 text-center mx-md-auto mb-5">
          <span class="text-cap">What is AriaPay?</span>
          <h2>AriaPay takes the stress out of service charges collection </h2>
